@model IEnumerable<Major>

<ul class="nav">
    @foreach(var m in Model)
    {
        <li class="nav-item" id="@m.MajorID">
            <a class="nav-link" style="cursor: pointer;">@m.MajorName</a>
        </li>
    }
    <li class="nav-item" id="0">
        <a class="nav-link" style="cursor: pointer;">BackToListDefault</a>
    </li>
</ul>
